<p id="notice"><%= notice %></p>

<h1>INFORME MEDICO PARTE DIARIO FISIOTERAPIA</h1>

<%= link_to 'Nuevo parte fisioterapia', new_parte_fisioterapium_path %>

<table>
  <thead>
    <tr>
      <th>Turno</th>
      <th>Mes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parte_fisioterapia.each do |parte_fisioterapium| %>
      <tr>
        <td><%= parte_fisioterapium.turno %></td>
        <td><%= parte_fisioterapium.mes %></td>
        <td><%= link_to 'Mostrar', parte_fisioterapium %></td>
        <td><%= link_to 'Editar', edit_parte_fisioterapium_path(parte_fisioterapium) %></td>
        <td><%= link_to 'Eliminar', parte_fisioterapium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


