<p id="notice"><%= notice %></p>

<h1>Parte Diarios</h1>

<table>
  <thead>
    <tr>
      <th>Mes</th>
      <th>Servicio de</th>
      <th>Nombre medico</th>
      <th>Clave</th>
      <th>Inicio consulta</th>
      <th>Fin consulta</th>
      <th>Enfermera</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parte_diarios.each do |parte_diario| %>
      <tr>
        <td><%= parte_diario.mes %></td>
        <td><%= parte_diario.servicio_de %></td>
        <td><%= parte_diario.nombre_medico %></td>
        <td><%= parte_diario.clave %></td>
        <td><%= parte_diario.inicio_consulta %></td>
        <td><%= parte_diario.fin_consulta %></td>
        <td><%= parte_diario.enfermera %></td>
        <td><%= link_to 'Show', parte_diario %></td>
        <td><%= link_to 'Edit', edit_parte_diario_path(parte_diario) %></td>
        <td><%= link_to 'Destroy', parte_diario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Parte Diario', new_parte_diario_path %>
