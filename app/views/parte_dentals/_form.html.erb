<%= form_for(parte_dental) do |f| %>
  <% if parte_dental.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(parte_dental.errors.count, "error") %> prohibited this parte_dental from being saved:</h2>

      <ul>
      <% parte_dental.errors.full_messages.each do |message| %>
