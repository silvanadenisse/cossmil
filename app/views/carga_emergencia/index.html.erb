<p id="notice"><%= notice %></p>

<h1>Carga Emergencia</h1>

<table>
  <thead>
    <tr>
      <th>Dias habiles</th>
      <th>Mes</th>
      <th>Anho</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @carga_emergencia.each do |carga_emergencium| %>
      <tr>
        <td><%= carga_emergencium.dias_habiles %></td>
        <td><%= carga_emergencium.mes %></td>
        <td><%= carga_emergencium.anho %></td>
        <td><%= link_to 'Show', carga_emergencium %></td>
        <td><%= link_to 'Edit', edit_carga_emergencium_path(carga_emergencium) %></td>
        <td><%= link_to 'Destroy', carga_emergencium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

