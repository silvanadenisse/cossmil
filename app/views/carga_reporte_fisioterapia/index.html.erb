<p id="notice"><%= notice %></p>

<h1>Carga Reporte Fisioterapia</h1>

<br><%= link_to 'Nuevo Carga Reporte Fisioterapia', new_carga_reporte_fisioterapium_path %></br>

<table>
  <thead>
    <tr>
      <th>Médicos</th>
      <th>Carga horaria</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @carga_reporte_fisioterapia.each do |carga_reporte_fisioterapium| %>
      <tr>
        <td><%= carga_reporte_fisioterapium.users %></td>
        <td><%= carga_reporte_fioterapium.carga_fisioterapia %></td>
        <td><%= link_to 'Mostrar', carga_reporte_fisioterapium %></td>
        <td><%= link_to 'Editar', edit_carga_reporte_fisioterapium_path(carga_reporte_fisioterapium) %></td>
        <td><%= link_to 'Eliminar', carga_reporte_fisioterapium, method: :delete, data: { confirm: 'Estás seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


