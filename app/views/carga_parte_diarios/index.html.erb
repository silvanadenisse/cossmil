<p id="notice"><%= notice %></p>

<h1>Carga Parte Diarios</h1>

<table>
  <thead>
    <tr>
      <th>Dias habiles</th>
      <th>Mes</th>
      <th>Anho</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @carga_parte_diarios.each do |carga_parte_diario| %>
      <tr>
        <td><%= carga_parte_diario.dias_habiles %></td>
        <td><%= carga_parte_diario.mes %></td>
        <td><%= carga_parte_diario.anho %></td>
        <td><%= link_to 'Show', carga_parte_diario %></td>
        <td><%= link_to 'Edit', edit_carga_parte_diario_path(carga_parte_diario) %></td>
        <td><%= link_to 'Destroy', carga_parte_diario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

