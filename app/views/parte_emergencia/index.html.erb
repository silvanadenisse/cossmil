<p id="notice"><%= notice %></p>

<h1>Parte Emergencia</h1>

<table>
  <thead>
    <tr>
      <th>Mes</th>
      <th>Enfermera</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parte_emergencia.each do |parte_emergencium| %>
      <tr>
        <td><%= parte_emergencium.mes %></td>
        <td><%= parte_emergencium.enfermera %></td>
        <td><%= link_to 'Show', parte_emergencium %></td>
        <td><%= link_to 'Edit', edit_parte_emergencium_path(parte_emergencium) %></td>
        <td><%= link_to 'Destroy', parte_emergencium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

