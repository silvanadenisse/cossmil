<p id="notice"><%= notice %></p>

<h1>Carga Reporte Emergencia</h1>

<table>
  <thead>
    <tr>
      <th>Medicos</th>
      <th>Carga horaria</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @carga_reporte_emergencia.each do |carga_reporte_emergencium| %>
      <tr>
        <td><%= carga_reporte_emergencium.users %></td>
        <td><%= carga_reporte_emergencium.carga_emergencia %></td>
        <td><%= link_to 'Mostrar', carga_reporte_emergencium %></td>
        <td><%= link_to 'Editar', edit_carga_reporte_emergencium_path(carga_reporte_emergencium) %></td>
        <td><%= link_to 'Destroy', carga_reporte_emergencium, method: :delete, data: { confirm: 'Estas seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Carga Reporte Emergencium', new_carga_reporte_emergencium_path %>
